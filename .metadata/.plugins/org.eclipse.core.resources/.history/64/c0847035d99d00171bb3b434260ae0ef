
    drop table CHARGER cascade constraints;

    create table CHARGER (
        ID varchar2(42 char) not null,
        STATIONID varchar2(42 char),
        POWERLEFT float,
        CHARGERSTATUS varchar2(255 char),
        CHARGERCAPACITY float,
        BATTERYTYPE varchar2(255 char),
        DEPLOYDATE varchar2(255 char),
        MANUFACTURER varchar2(255 char),
        REMARK varchar2(255 char),
        primary key (ID)
    );

    comment on table CHARGER is
        'Charger';

    comment on column CHARGER.ID is
        'chargerID';

    comment on column CHARGER.STATIONID is
        'stationID';

    comment on column CHARGER.POWERLEFT is
        'powerLeft';

    comment on column CHARGER.CHARGERSTATUS is
        '0  1  2  ';

    comment on column CHARGER.CHARGERCAPACITY is
        'chargerCapacity';

    comment on column CHARGER.BATTERYTYPE is
        'batteryType';

    comment on column CHARGER.DEPLOYDATE is
        'deployDate';

    comment on column CHARGER.MANUFACTURER is
        'manufacturer';

    comment on column CHARGER.REMARK is
        '备注';
